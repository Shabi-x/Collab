{
  "installCommand": "pnpm install",
  "outputDirectory": "build",
  "buildCommand": "npm run build:vercel",
  "builds": [
    { "src": "build/core.js", "use": "@vercel/node" },
    { "src": "build/static/*.*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/core.js"
    }
  ]
}
